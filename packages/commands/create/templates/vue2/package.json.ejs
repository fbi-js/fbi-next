{
  "name": "<%= create.name %>",
  "version": "0.0.0",
  "description": "<%= create.description %>",
  "private": true,
  "scripts": {
    "dev": "fbi serve",
    "build": "fbi build",
    "format": "npx fbi format"
  },
  "dependencies": {
    "vue": "^2.6.12",
    "vue-router": "^3.4.9"
  },
  "devDependencies": {
    "@fbi-js/cli": "^5.0.0-alpha.0",
    "@fbi-js/eslint-config-vue-typescript": "^1.2.2",
    "@fbi-js/format": "^1.0.0",
    "typescript": "^4.1.3"
  },
  "fbi": {
    "create": {
      "template": "<%= create.template %>",
      "features": <%- JSON.stringify(create.features) %>
    },
    "format": {
      "pattern": "src/**/*"
    }
  },
  "stylelint": {
    "extends": ["@fbi-js/stylelint-config"]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "eslintConfig": {
    "extends": [
      "@fbi-js/eslint-config-vue-typescript"
    ]
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "endOfLine": "lf",
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "always",
    "vueIndentScriptAndStyle": true
  }
}
